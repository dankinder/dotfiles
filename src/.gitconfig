[user]
        name = dkinder
        email = dkinder.is.me@gmail.com
[color]
        branch = auto
        diff = auto
        grep = auto
        interactive = auto
        showbranch = auto
        status = auto
        ui = auto
[core]
        trustctime = false
	excludesfile = ~/.cvsignore
[diff]
        mnemonicprefix = true
[alias]
        st = status
        ci = commit
        grep = grep --color=auto
        br = branch
        co = checkout
[branch]
        autosetuprebase = always
[interactive]
        singlekey = true
[push]
        default = current
