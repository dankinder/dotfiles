# iParadigms kvm proxy
Host dvm* kvm* tvm* cvm* cl6* cl5* tl6* tl5* jenkins
  HostName %h.local
  ProxyCommand /usr/bin/nc -X 5 -x ip-cpod1.oak.iparadigms.com:1080 %h %p

Host *
  StrictHostKeyChecking no
  CheckHostIP yes
  ForwardAgent yes
  GSSAPIAuthentication no
  ServerAliveInterval 30
